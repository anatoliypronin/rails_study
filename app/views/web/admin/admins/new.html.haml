= simple_form_for @admin, url: admin_admins_path do |admin|
  = admin.input :name
  = admin.input :email
  = admin.input :role
  = admin.input :password
  = admin.button :submit, t('.create_admin'), class: 'btn btn-outline-secondary'
  = link_to t('.cancel'), admin_admins_path, class: 'btn btn-outline-secondary'